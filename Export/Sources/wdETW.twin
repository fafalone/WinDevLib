    '         to be search for decoding information when decoding an event using
    '         TdhGetWppProperty or TdhGetWppMessage. (Not used by TdhGetProperty
    '         or TdhGetEventInformation.) */
        TDH_CONTEXT_MAXIMUM
    End Enum

