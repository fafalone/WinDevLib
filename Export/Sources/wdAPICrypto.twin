        CRYPT_VERIFY_DATA_HASH = &H00000040
    '  Time Valid Object flags
        CRYPT_KEEP_TIME_VALID = &H00000080
        CRYPT_DONT_VERIFY_SIGNATURE = &H00000100
        CRYPT_DONT_CHECK_TIME_VALIDITY = &H00000200
