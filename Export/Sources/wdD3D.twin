#Region "Macros"
Public Function D3D12_ENCODE_BASIC_FILTER(min As Long, mag As Long, mip As Long, reduction As Long) As Long
    D3D12_ENCODE_BASIC_FILTER = ((min And D3D12_FILTER_TYPE_MASK) << D3D12_MIN_FILTER_SHIFT) Or _
                                ((mag And D3D12_FILTER_TYPE_MASK) << D3D12_MAG_FILTER_SHIFT) Or _
                                ((mip And D3D12_FILTER_TYPE_MASK) << D3D12_MIP_FILTER_SHIFT) Or _
                                ((reduction And D3D12_FILTER_REDUCTION_TYPE_MASK) << D3D12_FILTER_REDUCTION_TYPE_SHIFT)
End Function

' Function to encode the anisotropic filter
Public Function D3D12_ENCODE_ANISOTROPIC_FILTER(reduction As Long) As Long
    D3D12_ENCODE_ANISOTROPIC_FILTER = D3D12_ANISOTROPIC_FILTERING_BIT Or _
                                      D3D12_ENCODE_BASIC_FILTER(D3D12_FILTER_TYPE_LINEAR, _
                                                                D3D12_FILTER_TYPE_LINEAR, _
                                                                D3D12_FILTER_TYPE_LINEAR, _
                                                                reduction)
End Function

' Function to encode the min mag anisotropic mip point filter
Public Function D3D12_ENCODE_MIN_MAG_ANISOTROPIC_MIP_POINT_FILTER(reduction As Long) As Long
    D3D12_ENCODE_MIN_MAG_ANISOTROPIC_MIP_POINT_FILTER = D3D12_ANISOTROPIC_FILTERING_BIT Or _
                                                        D3D12_ENCODE_BASIC_FILTER(D3D12_FILTER_TYPE_LINEAR, _
                                                                                  D3D12_FILTER_TYPE_LINEAR, _
                                                                                  D3D12_FILTER_TYPE_POINT, _
                                                                                  reduction)
End Function

Public Function D3D12_DECODE_MIN_FILTER(D3D12Filter As Long) As Long
    D3D12_DECODE_MIN_FILTER = (D3D12Filter \ 2 ^ D3D12_MIN_FILTER_SHIFT) And D3D12_FILTER_TYPE_MASK
End Function

' Function to decode the mag filter type from the filter
Public Function D3D12_DECODE_MAG_FILTER(D3D12Filter As Long) As Long
    D3D12_DECODE_MAG_FILTER = (D3D12Filter \ 2 ^ D3D12_MAG_FILTER_SHIFT) And D3D12_FILTER_TYPE_MASK
End Function

' Function to decode the mip filter type from the filter
Public Function D3D12_DECODE_MIP_FILTER(D3D12Filter As Long) As Long
    D3D12_DECODE_MIP_FILTER = (D3D12Filter \ 2 ^ D3D12_MIP_FILTER_SHIFT) And D3D12_FILTER_TYPE_MASK
End Function

' Function to decode the filter reduction type from the filter
Public Function D3D12_DECODE_FILTER_REDUCTION(D3D12Filter As Long) As Long
    D3D12_DECODE_FILTER_REDUCTION = (D3D12Filter \ 2 ^ D3D12_FILTER_REDUCTION_TYPE_SHIFT) And D3D12_FILTER_REDUCTION_TYPE_MASK
End Function

' Function to check if the filter is a comparison filter
Public Function D3D12_DECODE_IS_COMPARISON_FILTER(D3D12Filter As Long) As Boolean
    D3D12_DECODE_IS_COMPARISON_FILTER = (D3D12_DECODE_FILTER_REDUCTION(D3D12Filter) = D3D12_FILTER_REDUCTION_TYPE_COMPARISON)
End Function

Public Function D3D12_DECODE_IS_ANISOTROPIC_FILTER(D3D12Filter As Long) As Boolean
    D3D12_DECODE_IS_ANISOTROPIC_FILTER = ((D3D12Filter And D3D12_ANISOTROPIC_FILTERING_BIT) <> 0) And _
                                         (D3D12_FILTER_TYPE_LINEAR = D3D12_DECODE_MIN_FILTER(D3D12Filter)) And _
                                         (D3D12_FILTER_TYPE_LINEAR = D3D12_DECODE_MAG_FILTER(D3D12Filter))
End Function


#End Region
End Module