        fAngleInRadian = fDegreeY * (3.141593! / 180!)
    
        D2D1SinCos fAngleInRadian, fSin, fCos
    
        With Matrix4x4F_RotationY
    
        .m_11 = fCos
        .m_13 = -fSin
        .m_22 = 1!
        .m_31 = fSin
        .m_33 = fCos
        .m_44 = 1!

        End With
    
    End Function

