'slerror.h
Public Const SL_MDOLLAR_ZONE  = &HA000&  ' zone for LM server errors
Public Const SL_SERVER_ZONE  = &HB000&  ' zone for SL server errors
Public Const SL_MSCH_ZONE  = &HC000&  ' zone for SL clearing house errors
'  0XD000 is reserved for SP
Public Const SL_INTERNAL_ZONE  = &HE000&  ' zone for SL service internal errors
Public Const SL_CLIENTAPI_ZONE  = &HF000&  ' zone for SL client API errors

    Public Function IS_SL_SERVER_ERROR(ByVal hr As Long) As Boolean
        Return ((hr And &HF000&) = SL_SERVER_ZONE)
    End Function

    Public Function IS_SL_MSCH_ERROR(ByVal hr As Long) As Boolean
        Return ((hr And &HF000&) = SL_MSCH_ZONE)
    End Function

    Public Function IS_SL_INTERNAL_ERROR(ByVal hr As Long) As Boolean
        Return ((hr And &HF000&) = SL_INTERNAL_ZONE)
    End Function

    ' Public Function IS_SL_SERVICE_ERROR(ByVal hr As Long) As Boolean
    '     Return ((hr And &HF000&) = SL_SERVICE_ZONE)
    ' End Function

    Public Function IS_SL_CLIENTAPI_ERROR(ByVal hr As Long) As Boolean
        Return ((hr And &HF000&) = SL_CLIENTAPI_ZONE)
    End Function

    Public Function IS_SL_MDOLLAR_ERROR(ByVal hr As Long) As Boolean
        Return ((hr And &HF000&) = SL_MDOLLAR_ZONE)
    End Function

    Public Function IS_SL_VGA_STRICT_ERROR(ByVal hr As Long) As Boolean
        Return ((hr And &HF000&) = SL_MSCH_ZONE) And ((hr And &HFFF&) >= &H401&) And ((hr And &HFFF&) <= &H496&)
    End Function

    Public Function IS_SL_VGA_LITE_ERROR(ByVal hr As Long) As Boolean
        Return ((hr And &HF000&) = SL_MSCH_ZONE) And ((hr And &HFFF&) >= &H497&) And ((hr And &HFFF&) <= &H600&)
    End Function

    Public Function IS_STORE_TOKEN_ERROR_RANGE(ByVal hr As Long) As Boolean
        Return (SL_E_STORE_UPGRADE_TOKEN_REQUIRED <= hr) And (SL_E_STORE_UPGRADE_TOKEN_NOT_AUTHORIZED >= hr)
    End Function

