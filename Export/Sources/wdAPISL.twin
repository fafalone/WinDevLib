Public Function WINDOWS_SLID() As UUID
Static iid As UUID
	If (iid.Data1 = 0) Then Call DEFINE_UUID(iid, &H55c92734, &Hd682, &H4d71, &H98, &H3e, &Hd6, &Hec, &H3f, &H16, &H05, &H9f)
WINDOWS_SLID = iid
End Function
    
