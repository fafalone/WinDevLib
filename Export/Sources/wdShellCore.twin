    PixelFormat64bppPARGB = (14 Or (64 << 8) Or PixelFormatAlpha Or PixelFormatPAlpha Or PixelFormatExtended)
    PixelFormat32bppCMYK = (15 Or (32 << 8))
    PixelFormatMax = 16
End Enum

