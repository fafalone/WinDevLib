    PixelFormat32bppRGB = (9 Or (32 << 8) Or PixelFormatGDI)
    PixelFormat32bppARGB = (10 Or (32 << 8) Or PixelFormatAlpha Or PixelFormatGDI Or PixelFormatCanonical)
