        IP_DEST_HOST_UNREACHABLE = (IP_STATUS_BASE + 3)
        IP_DEST_PROT_UNREACHABLE = (IP_STATUS_BASE + 4)
        IP_DEST_PORT_UNREACHABLE = (IP_STATUS_BASE + 5)
