Public Enum CryptCertRdn
    CERT_RDN_ANY_TYPE  = 0
    CERT_RDN_ENCODED_BLOB  = 1
    CERT_RDN_OCTET_STRING  = 2
    CERT_RDN_NUMERIC_STRING  = 3
    CERT_RDN_PRINTABLE_STRING = 4
    CERT_RDN_TELETEX_STRING  = 5
    CERT_RDN_T61_STRING  = 5
