Public Enum ServiceType
	SERVICE_KERNEL_DRIVER = &H00000001
	SERVICE_FILE_SYSTEM_DRIVER  = &H00000002
	SERVICE_ADAPTER  = &H00000004
	SERVICE_RECOGNIZER_DRIVER  = &H00000008
