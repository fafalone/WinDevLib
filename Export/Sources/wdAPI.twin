    SPSVCINST_NOCLOBBER_SERVICESIDTYPE  = (&H00004000)
'  (AddService) don't overwrite delayed auto start if it already exists
    SPSVCINST_NOCLOBBER_DELAYEDAUTOSTART  = (&H00008000&)
