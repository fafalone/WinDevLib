Public Function ILIsEmpty(ByVal pidl As LongPtr) As BOOL
If pidl = 0 Then Return CTRUE
Dim cb As Integer
CopyMemory cb, ByVal pidl, 2
If cb = 0 Then Return CTRUE
End Function
